{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "lua",
      "$VIMRUNTIME",
      "${3rd}/luv/library",
      "$HOME/.local/share/nvim/site/pack/deps/start/mini.nvim",
    ],
    "checkThirdParty": false
  },
  "diagnostics.globals": [
    "MiniDeps"
  ]
}
